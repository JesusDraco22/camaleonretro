<html lang="es">
<head>
    
    <style>
        body {
            font-family: Arial, sans-serif;
            line-height: 1.6;
            margin: 20px;
            background-color: #f2f2f2;
        }
        header {
            background-color: #F9F9F9;
            color: #fff;
            padding: 10px 20px;
            text-align: center;
        }
        #map {
            height: 400px;
            width: 100%;
        }
        footer {
            background-color: #333;
            color: #fff;
            text-align: center;
            padding: 10px 0;
            position: absolute;
            bottom: 0;
            width: 100%;
        }
    </style>
</head>
<body>
    <header>
        <h1>Ubicación de Nuestro Lugar</h1>
    </header>
    <div id="map"></div>
<script>
        // Función para inicializar y mostrar el mapa
        function initMap() {
            // Coordenadas del lugar (ejemplo: Nueva York)
            var lugar = { lat: 40.712776, lng: -74.005974 };

            // Crear un nuevo mapa en el elemento con id 'map'
            var map = new google.maps.Map(document.getElementById('map'), {
                center: lugar,
                zoom: 15 // Nivel de zoom (1 = Mundo, 20 = Calle detallada)
            });

            // Crear un marcador en la ubicación del lugar
            var marker = new google.maps.Marker({
                position: lugar,
                map: map,
                title: 'Ubicación de Nuestro Lugar'
            });
        }
    </script>
    <script async defer
        src="https://www.google.es/maps/dir//Ica+11004/@-14.0765308,-75.8216295,12z/data=!4m8!4m7!1m0!1m5!1m1!1s0x9110e2ed1206144f:0xb7487523ad25c1a!2m2!1d-75.739237!2d-14.0765445?entry=ttu">
    </script>
</body>
</html>

